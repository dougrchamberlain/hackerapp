{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "eofline": true,
        "semicolon": [
            true,
            "always"
        ],
        "space-within-parens": true,
        "no-var-requires": false,
        "trailing-comma": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}